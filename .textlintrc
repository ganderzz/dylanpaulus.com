{
  "rules": {
    "period-in-list-item": true,
    "@textlint-rule/no-unmatched-pair": true,
    "footnote-order": true,
    "rousseau": {
      "showLevels": [
        "suggestion",
        "warning",
        "error"
      ]
    },
    "stop-words": true,
    "no-start-duplicated-conjunction": {
      "interval": 2
    },
    "max-comma": {
      "max": 5
    }
  }
}
